    Private Sub btnBrowse_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnBrowse.Click

        Dim Dialog As New FolderBrowserDialog
        Dialog.ShowNewFolderButton = False
        Dialog.Description = "Select a folder:"
        lblStatue.Text = "Please select a folder to import files from"
        If Dialog.ShowDialog = Windows.Forms.DialogResult.OK Then
            lblStatue.Text = "Target selected. Checking..."
            txtPath.Text = Dialog.SelectedPath
            Import(Dialog.SelectedPath)
        Else
            lblStatue.Text = "Importing canceled."
        End If
        Dialog.Dispose()

    End Sub

    Private Sub Import(ByVal Path As String)
        If IO.Directory.Exists(Path) = False Then
            lblStatue.Text = "Path does not exist."
            MessageBox.Show("The path does not exist", "Importing a folder", MessageBoxButtons.OK, MessageBoxIcon.Exclamation)
            Exit Sub
        End If
Retry:  Files = IO.Directory.GetFiles(Path).Where(Function(i As String) ((IO.File.GetAttributes(i) And IO.FileAttributes.Hidden) <> IO.FileAttributes.Hidden)).ToArray
        If Files.Count = 0 Then
            lblStatue.Text = "The folder you selected does not contain non-hidden files."
            MessageBox.Show("The folder you selected does not contain non-hidden files.", "Importing a folder", MessageBoxButtons.OK, MessageBoxIcon.Exclamation)
            Exit Sub
        End If
        Try
            My.Computer.FileSystem.WriteAllText(My.Computer.FileSystem.CombinePath(Path, "Temp.tmp"), "Temp File. you can delete this file safely", False)
            My.Computer.FileSystem.DeleteFile(My.Computer.FileSystem.CombinePath(Path, "Temp.tmp"))

        Catch ex As Exception
            lblStatue.Text = "Access is denied."
            If MessageBox.Show("Access to the folder is denied. Make sure you have administrator access", "Importing a folder", MessageBoxButtons.RetryCancel, MessageBoxIcon.Exclamation) = Windows.Forms.DialogResult.Retry Then GoTo Retry Else Exit Sub
        End Try
        lblStatue.Text = "Folder checked succsesfully. Importing..."
        If lstFiles.Items.Count > 0 Then
            If MessageBox.Show("Are You Sure You Want To Clear Listbox And Import New Files?", "Importing a folder", MessageBoxButtons.YesNo, MessageBoxIcon.Question) = Windows.Forms.DialogResult.No Then Exit Sub
        End If
        lstFiles.Items.Clear()
        For Each i As String In Files
            lstFiles.Items.Add(IO.Path.GetFileName(i))
        Next
        lblStatue.Text = "Folder imported succsesfully."
    End Sub

    Private Sub btnStart_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnStart.Click

        If lstFiles.Items.Count < 1 Then Exit Sub
        If MessageBox.Show("Are you sure you want to start renaming?", "Renaming", MessageBoxButtons.YesNo, MessageBoxIcon.Question, MessageBoxDefaultButton.Button2) = Windows.Forms.DialogResult.No Then Exit Sub
        If txtIndex.Text = "" Then txtIndex.Text = " "
        lblStatue.Text = "Preparing files..."
        prgProgress.Value = 0
        prgProgress.Maximum = CInt(2.75 * (Files.Count - 1))
        prgProgress.Step = 1
        Randomize()
        Dim rand As New Random
        Dim letter As String
        letter = ChrW(rand.Next(Asc("A"), Asc("Z") + 1)) & ChrW(rand.Next(Asc("A"), Asc("Z") + 1)) & ChrW(rand.Next(Asc("A"), Asc("Z") + 1))

        For Counter As Integer = 0 To Files.Count - 1
            Dim i As Integer = Counter
            Dim CurrentItem As String = IO.Directory.GetFiles(GetDirectoryName(Files(0))).Where(Function(obj As String) ((IO.File.GetAttributes(obj) And IO.FileAttributes.Hidden) <> IO.FileAttributes.Hidden) And obj.Contains(CStr(lstFiles.Items(i))))(0)
            Dim Text As String = If(chkIndex.Checked,
                                    CStr(i + CInt(nmrStartFrom.Value)) & txtIndex.Text & GetFileNameWithoutExtension(CurrentItem) & "." & letter & GetExtension(CurrentItem),
                                    CStr(i + CInt(nmrStartFrom.Value)) & "." & letter & GetExtension(CurrentItem))
Retry1:     Try
                My.Computer.FileSystem.RenameFile(CurrentItem, Text)
                lstFiles.Items(i) = Text
                lblStatue.Text = (i + 1) & " File(s) done, " & Files.Count - (i + 1) & " To go, preparing..."

            Catch ex1 As Exception
                lblStatue.Text = "Preparing faild."
                Dim ex As New Exception(Replace(ex1.Message, {"Argument cannot be Nothing"}, "The file had been renamed after that it's imported"), ex1.InnerException)
                Select Case MessageBox.Show("Cannot continue renaming. " & ex.Message, "Renaming progress", MessageBoxButtons.AbortRetryIgnore, MessageBoxIcon.Hand)
                    Case Windows.Forms.DialogResult.Retry
                        lblStatue.Text = "Retring..."

                        GoTo Retry1
                    Case Windows.Forms.DialogResult.Ignore
                        lblStatue.Text = "File ingored. Preparing another file..."

                        Exit Try
                    Case Else
                        lblStatue.Text = "Renaming aborted."

                        Exit Sub
                End Select
            End Try
            prgProgress.PerformStep()

        Next
        lblStatue.Text = "Files Prepared. Renaming files..."
        For i As Integer = 0 To CInt(0.5 * (Files.Count - 1))
            prgProgress.PerformStep()
            Me.Refresh()
        Next
        Dim l As Integer = -1
        For Each CurrentItem As String In IO.Directory.GetFiles(GetDirectoryName(Files(0))).Where(Function(obj As String) ((IO.File.GetAttributes(obj) And IO.FileAttributes.Hidden) <> IO.FileAttributes.Hidden))
            l = l + 1
Retry2:     Try
                My.Computer.FileSystem.RenameFile(CurrentItem, GetFileNameWithoutExtension(GetFileNameWithoutExtension(CurrentItem)) & GetExtension(CurrentItem))
                lblStatue.Text = (l + 1) & " File(s) done, " & Files.Count - (l + 1) & " To go, renaming..."

            Catch ex As Exception
                Select Case MessageBox.Show("Cannot continue renaming. " & ex.Message, "Renaming progress", MessageBoxButtons.AbortRetryIgnore, MessageBoxIcon.Hand)
                    Case Windows.Forms.DialogResult.Retry
                        lblStatue.Text = "Retring..."

                        GoTo Retry2
                    Case Windows.Forms.DialogResult.Ignore
                        lblStatue.Text = "File ingored. Renaming another file..."

                        Exit Try
                    Case Else
                        lblStatue.Text = "Renaming aborted."

                        Exit Sub
                End Select
            End Try
            prgProgress.PerformStep()

        Next
        lblStatue.Text = "Refreshing list..."

        Files = IO.Directory.GetFiles(GetDirectoryName(Files(0))).Where(Function(i As String) ((IO.File.GetAttributes(i) And IO.FileAttributes.Hidden) <> IO.FileAttributes.Hidden)).ToArray
        lstFiles.Items.Clear()
        For Each i As String In Files
            lstFiles.Items.Add(IO.Path.GetFileName(i))
        Next
        For i As Integer = 0 To CInt(0.25 * (Files.Count - 1))
            prgProgress.PerformStep()
            Me.Refresh()
        Next
        lblStatue.Text = "Done renaming."
        MsgBox("Done renaming", MsgBoxStyle.Information)
        prgProgress.Value = 0

    End Sub

    Private Sub btnUp_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnUp.Click
        If lstFiles.SelectedIndex < 1 Then Exit Sub
        Dim List As List(Of String) = Files.ToList
        Dim Index As Integer = lstFiles.SelectedIndex
        Dim Item As String = List(Index)
        If Index < 1 Then Exit Sub

        List.RemoveAt(Index)
        List.Insert(Index - 1, Item)
        Files = List.ToArray
        lstFiles.Items.Clear()
        For Each i As String In Files
            lstFiles.Items.Add(IO.Path.GetFileName(i))
        Next
        lstFiles.SelectedIndex = Index - 1
        lblStatue.Text = "Item moved up."
    End Sub
    Private Sub btnDown_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnDown.Click
        If lstFiles.SelectedIndex < 1 Then Exit Sub
        Dim List As List(Of String) = Files.ToList
        Dim Index As Integer = lstFiles.SelectedIndex
        Dim Item As String = List(Index)
        If Index >= (lstFiles.Items.Count - 1) Then Exit Sub

        List.RemoveAt(Index)
        List.Insert(Index + 1, Item)
        Files = List.ToArray
        lstFiles.Items.Clear()
        For Each i As String In Files
            lstFiles.Items.Add(IO.Path.GetFileName(i))
        Next
        lstFiles.SelectedIndex = Index + 1
        lblStatue.Text = "Item moved down."
    End Sub

    Private Sub lstFiles_DoubleClick(ByVal sender As Object, ByVal e As System.EventArgs) Handles lstFiles.DoubleClick
        lblStatue.Text = "Openning..."
        If lstFiles.SelectedItem IsNot Nothing Then
            Dim str As String = Files(lstFiles.SelectedIndex)
            Process.Start(str)
        End If
        lblStatue.Text = "File opened."
    End Sub

