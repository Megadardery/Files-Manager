    Private Sub txtIndex_TextChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles txtIndex.TextChanged
        If txtIndex.Text <> Replace(txtIndex.Text, IO.Path.GetInvalidFileNameChars, "") Then
            lblStatue.Text = "Unallowed character"
            MessageBox.Show("A file name cannot contain any of those characters: \ / : * ? "" < > |", "Unallowed characters", MessageBoxButtons.OK, MessageBoxIcon.Exclamation)
            txtIndex.Text = Replace(txtIndex.Text, IO.Path.GetInvalidFileNameChars, "")
            txtIndex.Select(txtIndex.Text.Length, 0)
        End If


    End Sub

    Private Function Replace(ByVal Exprestion As String, ByVal Find() As Char, ByVal Replacement As String) As String
        For i As Integer = 0 To UBound(Find)
            Exprestion = Strings.Replace(Exprestion, Find(i), Replacement)
        Next
        Return Exprestion
    End Function
    Private Function Replace(ByVal Exprestion As String, ByVal Find() As String, ByVal Replacement As String) As String
        For i As Integer = 0 To UBound(Find)
            Exprestion = Strings.Replace(Exprestion, Find(i), Replacement)
        Next
        Return Exprestion
    End Function
